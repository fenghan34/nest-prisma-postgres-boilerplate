{
  "name": "nest-prisma-postgres-boilerplate",
  "private": true,
  "version": "0.1.0",
  "description": "Boilerplate for Nest.js-TypeORM-Postgres project",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "dev:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start": "cross-env NODE_ENV=production node dist/main",
    "build": "rimraf dist && nest build",
    "format": "prettier -w -u --loglevel=error .",
    "lint": "eslint . --fix --ext .js,.ts",
    "migrate": "dotenv -e config/.env.development -- pnpx prisma migrate dev --name postgres-init",
    "test": "jest --watch",
    "test:cov": "jest --watchAll --coverage",
    "test:e2e": "jest --watchAll --config ./test/jest-e2e.json",
    "test:ci": "jest --ci --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@nestjs/common": "^8.1.1",
    "@nestjs/core": "^8.1.1",
    "@nestjs/platform-express": "^8.1.1",
    "@prisma/client": "^3.8.0",
    "cross-env": "^7.0.3",
    "dotenv": "^11.0.0",
    "prisma": "^3.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@nestjs/cli": "^8.1.3",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^8.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "dotenv-cli": "^4.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.2",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^7.0.4",
    "jest": "^27.3.0",
    "lint-staged": "^12.1.4",
    "prettier": "^2.4.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "4.4.4",
    "webpack": "^5.65.0"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint",
    "*": "prettier -w -u --loglevel=error"
  },
  "engines": {
    "node": "16.x",
    "pnpm": "6.x"
  }
}
